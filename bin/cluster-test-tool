#!/bin/bash

cmd=`cat <<EOF
import os
from cluster_test_tool import main
if __name__ == '__main__':
    main.ctx.name = os.path.basename("$0")
    main.run(main.ctx)
EOF`


if which python3 &> /dev/null; then
    python3 -c "$cmd" "$@"
elif which python2 &> /dev/null; then
    python2 -c "$cmd" "$@"
fi
